{
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "outputDirectory": "dist",
  "framework": "vite",
  "scope": "neoSphere",
  "builds": [
    {
      "src": "./neoSphere/**",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "./neoSphere/dist",
        "buildCommand": "npm run build"
      }
    }
  ],
  "routes": [
    {
      "src": "/[^.]+",
      "dest": "/index.html"
    }
  ]
}
